{
  "name": "ssb-patchql-client",
  "version": "1.0.0",
  "description": "An example app that has feeds / threads / posts etc.",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live -- -t babelify ",
    "start-prod": "budo . -d --serve bundle.js --live -- -t babelify -p tinyify",
    "build": "browserify . -o bundle.js -p tinyify",
    "build-sass": "gulp sass",
    "test": "browserify test/index.js | node | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pietgeursen/ssb-patchql-client"
  },
  "author": "pietgeursen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/pietgeursen/ssb-patchql-client/issues"
  },
  "homepage": "https://github.com/pietgeursen/ssb-patchql-client",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "apollo-boost": "^0.3.1",
    "autoprefixer": "^9.1.5",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "budo": "^11.6.1",
    "classnames": "^2.2.6",
    "emoji-named-characters": "^1.0.2",
    "foundation-sites": "^6.5.1",
    "gh-pages": "^0.11.0",
    "graphql": "^14.2.1",
    "gulp": "^4.0.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.1",
    "match-all": "^1.2.5",
    "motion-ui": "~2.0.0",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-apollo": "^2.5.4",
    "react-dom": "^16.8.6",
    "react-infinite-scroller": "^1.2.4",
    "react-markdown": "^4.0.8",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "regex-emoji": "^2.0.7",
    "ssb-markdown": "^4.1.0",
    "ssb-ref": "^2.13.9",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1",
    "tinyify": "^2.5.0",
    "watchify": "^3.7.0"
  }
}
